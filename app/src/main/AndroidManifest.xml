<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.satugenggam.aplikasi1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.view.dashboard.DrawerActivity"></activity>
        <activity android:name=".ui.view.dashboard.LayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.view.settings.SettingsActivity" />
        <activity android:name=".ui.view.order.OrderActivity"></activity>
        <activity android:name=".ui.view.profil.ShopChangePasswordActivity" />
        <activity android:name=".ui.view.profil.ShopEditActivity" />
        <activity android:name=".ui.view.profil.ShopProfileActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>